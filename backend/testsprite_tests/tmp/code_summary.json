{
    "tech_stack": [
        "Java",
        "Spring Boot 3.2.1",
        "PostgreSQL",
        "QueryDSL",
        "Spring Security",
        "JJWT"
    ],
    "features": [
        {
            "name": "Auth API",
            "description": "Authentication endpoints including Dev Login for testing.",
            "files": [
                "src/main/java/com/sorin/glossary/domain/auth/presentation/AuthController.java",
                "src/main/java/com/sorin/glossary/global/security/JwtTokenProvider.java",
                "src/main/java/com/sorin/glossary/global/security/JwtFilter.java",
                "src/main/java/com/sorin/glossary/global/config/SecurityConfig.java"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/api/v1/auth/dev-login": {
                        "post": {
                            "summary": "Development Login",
                            "requestBody": {
                                "content": {
                                    "application/json": {
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "userId": {
                                                    "type": "string"
                                                },
                                                "role": {
                                                    "type": "string"
                                                }
                                            },
                                            "required": [
                                                "userId"
                                            ]
                                        }
                                    }
                                }
                            },
                            "responses": {
                                "200": {
                                    "description": "Login Success",
                                    "content": {
                                        "application/json": {
                                            "schema": {
                                                "type": "object",
                                                "properties": {
                                                    "accessToken": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Process API",
            "description": "Manage manufacturing processes.",
            "files": [
                "src/main/java/com/sorin/glossary/domain/process/presentation/ProcessController.java"
            ],
            "api_doc": {
                "paths": {
                    "/api/v1/processes": {
                        "get": {
                            "summary": "List all processes"
                        },
                        "post": {
                            "summary": "Create process"
                        }
                    },
                    "/api/v1/processes/{id}": {
                        "put": {
                            "summary": "Update process"
                        },
                        "delete": {
                            "summary": "Delete process"
                        }
                    }
                }
            }
        },
        {
            "name": "Term API",
            "description": "Manage glossary terms.",
            "files": [
                "src/main/java/com/sorin/glossary/domain/term/presentation/TermController.java"
            ],
            "api_doc": {
                "paths": {
                    "/api/v1/terms": {
                        "get": {
                            "summary": "Search terms"
                        },
                        "post": {
                            "summary": "Create term"
                        }
                    },
                    "/api/v1/terms/{id}": {
                        "get": {
                            "summary": "Get term detail"
                        },
                        "put": {
                            "summary": "Update term"
                        },
                        "delete": {
                            "summary": "Delete term"
                        }
                    }
                }
            }
        },
        {
            "name": "Suggestion API",
            "description": "Manage term suggestions.",
            "files": [
                "src/main/java/com/sorin/glossary/domain/suggestion/presentation/SuggestionController.java"
            ],
            "api_doc": {
                "paths": {
                    "/api/v1/suggestions": {
                        "post": {
                            "summary": "Create suggestion"
                        }
                    },
                    "/api/v1/suggestions/{id}/approve": {
                        "post": {
                            "summary": "Approve suggestion"
                        }
                    },
                    "/api/v1/suggestions/{id}/reject": {
                        "post": {
                            "summary": "Reject suggestion"
                        }
                    }
                }
            }
        },
        {
            "name": "Bookmark API",
            "description": "Manage user bookmarks.",
            "files": [
                "src/main/java/com/sorin/glossary/domain/bookmark/presentation/BookmarkController.java"
            ],
            "api_doc": {
                "paths": {
                    "/api/v1/bookmarks/terms/{termId}": {
                        "post": {
                            "summary": "Toggle bookmark"
                        }
                    },
                    "/api/v1/bookmarks/my": {
                        "get": {
                            "summary": "Get my bookmarked term IDs"
                        }
                    }
                }
            }
        },
        {
            "name": "Search Log API",
            "description": "Manage search history.",
            "files": [
                "src/main/java/com/sorin/glossary/domain/log/presentation/SearchLogController.java"
            ],
            "api_doc": {
                "paths": {
                    "/api/v1/logs/search/recent": {
                        "get": {
                            "summary": "Get recent search logs"
                        }
                    }
                }
            }
        }
    ]
}